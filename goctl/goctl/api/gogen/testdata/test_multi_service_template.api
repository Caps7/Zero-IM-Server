type Request struct {
    Name string `path:"name,options=you|me"`
}

type Response struct {
    Message string `json:"message"`
}

service A-api {
    @server(
        handler: GreetHandler
    )
    get /greet/from/:name(Request) returns (Response)
}

service A-api {
    @server(
        handler: NoResponseHandler
    )
    get /greet/get(Request)
}